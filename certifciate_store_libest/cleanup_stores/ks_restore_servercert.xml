<keystore xmlns="urn:ietf:params:xml:ns:yang:ietf-keystore">
<asymmetric-keys>
     <asymmetric-key>
        <name>serverkey</name>
        <algorithm>secp256r1</algorithm>
        <public-key></public-key>
        <private-key></private-key>
        <certificates>
          <certificate>
            <name>servercert</name>
            <cert></cert>
          </certificate>
        </certificates>
      </asymmetric-key> 
</asymmetric-keys>
</keystore>
